body {
  /*font-family: 'Kalam', cursive;*/
  font-family: 'Dancing Script', cursive;
  overflow-x: hidden;
  background-position: bottom;
  background-attachment: fixed;
  background-image: url("/Content/images/blairavery3_top.png"); }

.btn,
button {
  font-family: inherit;
  outline: none;
  border: none;
  box-shadow: none; }
  .btn.active,
  button.active {
    outline: none;
    border: none;
    box-shadow: none; }
  .btn:hover,
  button:hover {
    outline: none;
    border: none;
    box-shadow: none; }
  .btn:focus,
  button:focus {
    outline: none;
    border: none;
    box-shadow: none; }

.spacer {
  height: 150px; }

.sub_sections {
  background-color: rgba(255, 255, 255, 0.8);
  padding: 20px; }

.section_title {
  padding: 0 50px; }
  .section_title h1 {
    padding: 0;
    padding-top: 5px;
    padding-bottom: 15px;
    border-bottom: 3px solid black; }

.section_row {
  padding-bottom: 50px;
  margin-bottom: 0px; }
  .section_row h1 {
    margin-bottom: 20px; }
  .section_row p {
    margin-top: 20px;
    margin-bottom: 20px; }
  .section_row a {
    margin-bottom: 10px;
    border: 2px solid black;
    border-radius: 0;
    width: 50%;
    color: black; }
    .section_row a:hover, .section_row a:focus, .section_row a.active {
      margin-bottom: 10px;
      border: 2px solid black;
      border-radius: 0;
      width: 50%;
      color: white;
      background-color: black; }
  .section_row .section_col_right {
    padding: 20px 50px;
    border-left: 3px solid black; }
  .section_row .section_col_left {
    padding: 20px 50px;
    border-right: 3px solid transparent; }
  .section_row img {
    height: 50px;
    width: 50px; }

.navbar {
  border-bottom: 1px solid black;
  background-color: #fff;
  height: 150px; }
  .navbar .countdown {
    display: none; }

.btn_rsvp_outer {
  position: relative;
  height: 750px; }
  .btn_rsvp_outer .btn_rsvp {
    position: absolute;
    padding: 25px 125px;
    top: 40%;
    left: calc(50% - 145px);
    font-size: 25px;
    background-color: rgba(255, 255, 255, 0.3);
    border: 3px solid #192231; }
    .btn_rsvp_outer .btn_rsvp:hover, .btn_rsvp_outer .btn_rsvp:focus, .btn_rsvp_outer .btn_rsvp.active {
      background-color: #192231;
      border: 3px solid #192231;
      color: white; }
  .btn_rsvp_outer .rsvp_answer {
    color: #192231;
    border: 1px solid #d3d3d3;
    padding: 12px 17px; }
    .btn_rsvp_outer .rsvp_answer.active {
      color: #fff;
      border: 0px solid #192231;
      background-color: #192231; }
    .btn_rsvp_outer .rsvp_answer:hover {
      cursor: pointer; }
  .btn_rsvp_outer .rsvp_container label {
    font-weight: 600; }
  .btn_rsvp_outer .rsvp_container .response_header {
    color: #494E6B; }
  .btn_rsvp_outer .rsvp_container .error {
    font-size: 12px;
    color: darkred; }
  .btn_rsvp_outer .rsvp_submit input {
    width: 100%;
    padding: 20px;
    background-color: #192231;
    /*border: 3px solid $color_blue;*/
    color: white; }

.guest_card .card-header {
  background-color: #fff;
  border: 2px solid #494E6B;
  border-bottom: 1px solid #494E6B; }

.guest_card .card-footer {
  background-color: #494E6B;
  color: #fff;
  border: 2px solid #494E6B;
  border-top: 0; }

.guest_card .card-block {
  border: 2px solid #494E6B;
  border-top: 0;
  border-bottom: 0; }

.admin_guest_container .sub_guest .details {
  margin: 0;
  padding: 5px 0; }

.admin_guest_container .sub_guest h6 {
  color: white;
  background-color: #d3d3d3;
  margin: 0;
  padding: 10px 0; }

.admin_guest_container .sub_guest .details_container {
  border: 1px solid #d3d3d3; }

.guest_section h3 {
  background-color: #d3d3d3;
  padding: 50px 0; }

.header_card .card-header {
  background-color: #192231;
  color: white; }

